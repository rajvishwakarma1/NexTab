{
    "name": "startpage",
    "version": "1.0.0",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "*://music.youtube.com/*",
        "*://*.scdn.co/*",
        "*://*.spotifycdn.com/*",
        "*://*.mzstatic.com/*",
        "*://*.googleusercontent.com/*",
        "*://*.ytimg.com/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' data: https: http:;"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "startpage@rajvishwakarma.tech"
        }
    }
}
